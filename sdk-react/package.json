{
  "name": "@fluxbase/sdk-react",
  "version": "0.1.0",
  "description": "React hooks for Fluxbase SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "keywords": [
    "fluxbase",
    "react",
    "hooks",
    "database",
    "realtime"
  ],
  "peerDependencies": {
    "@fluxbase/sdk": "^0.1.0",
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@fluxbase/sdk": "file:../sdk",
    "@tanstack/react-query": "^5.62.14",
    "@types/react": "^19.0.0",
    "tsup": "^8.3.5",
    "typedoc": "^0.28.14",
    "typescript": "^5.7.3"
  }
}
