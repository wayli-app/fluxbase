{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts"
  ],
  "out": "../docs/static/api/sdk",
  "name": "@fluxbase/sdk",
  "readme": "README.md",
  "plugin": [],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "includeVersion": true,
  "searchInComments": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "categorizeByGroup": true,
  "categoryOrder": [
    "Client",
    "Authentication",
    "Database",
    "Realtime",
    "Storage",
    "Types",
    "*"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/yourusername/fluxbase",
    "Documentation": "https://fluxbase.eu"
  }
}
